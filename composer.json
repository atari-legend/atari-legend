{
    "name": "atari-legend/atari-legend",
    "type": "project",
    "description": "Atari Legend",
    "keywords": [
        "atari"
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.0.2",
        "buzz/laravel-h-captcha": "^1.0",
        "doctrine/dbal": "^3.1",
        "embed/embed": "^4.3",
        "fruitcake/laravel-cors": "^2.0.5",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.5",
        "jbbcode/jbbcode": "^1.4",
        "laminas/laminas-diactoros": "^2.6",
        "laminas/laminas-feed": "^2.14",
        "laminas/laminas-http": "^2.14",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7.0",
        "laravel/ui": "^3.4",
        "livewire/livewire": "^2.10",
        "rappasoft/laravel-livewire-tables": "~1.23.0",
        "spatie/laravel-feed": "^4.1",
        "tizis/phpepub": "~5.1",
        "waynestate/parse-youtube-id": "^1.0",
        "webfontkit/roboto": "^1.0"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.1",
        "overtrue/phplint": "^4.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
